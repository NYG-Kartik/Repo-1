var popupEqnwin = null;
var curSelectedLaTeX = null;

function refreshLatexPreview(clicked) { // Refresh the preview image.
  var content_text = $('#latexTextArea').val();
  if (!clicked && tinymce.latexSearchCache == content_text)
    return false;
  if (content_text.trim().length == 0) {
    $('#latexRenderWrapper').html("");
    return false;
  }
  tinymce.latexSearchCache = content_text;
  $('#latexPreviewRender').text("$$" + content_text + "$$");
  P.renderer.renderMath('latexPreviewRender');
  return true;
};

function insertChem() {
  var myField = $("#latexTextArea")[0];
  var text = "\\ce{}";
  if (myField.selectionStart || myField.selectionStart == '0') {
    var startPos = myField.selectionStart;
    var endPos = myField.selectionEnd;
    myField.value = myField.value.substring(0, startPos)
        + text
        + myField.value.substring(endPos, myField.value.length);
    myField.selectionStart = startPos + 4;
    myField.selectionEnd = startPos + 4;
  } else {
    myField.value += text;
  }
  myField.focus();
}

function encodeImageSrc(latex, doEscape) { // Convert String to URI
  latex = latex.replace(/&lt;/g, "<");
  latex = latex.replace(/&gt;/g, ">");
  latex = latex.replace(/&amp;/g, "&");
  latex = latex.replace(/&quot;/g, '"');
  latex = latex.replace(/<br\s*[\/]?>>/gi, "\n");
  if (doEscape)
    return encodeURIComponent(latex);
  return latex;
};

tinymce.PluginManager.add('equation', function (ed) {

  ed.addCommand('insertfileCommand', function () {
    setTimeout(function () {
      P.modules.loadModule("upload_manager", {}, function () {
        PEM.fire("showUploadManager", {
          submit: function (url, name) {
            setTimeout(function () {
              ed.focus();
              if (typeof (actualCaretPositionBookmark) != 'undefined' && actualCaretPositionBookmark)
                ed.selection.moveToBookmark(actualCaretPositionBookmark);
              ed.insertContent(ed.dom.createHTML('a', {
                href: url,
                target: "_blank"
              }, name));
            }, 500);
          }, cancel: function () {
            setTimeout(function () {
              ed.focus();
              if (typeof (actualCaretPositionBookmark) != 'undefined' && actualCaretPositionBookmark)
                ed.selection.moveToBookmark(actualCaretPositionBookmark);
            }, 500);
          }
        });
      }, null);
    }, 100);
  });
  ed.addCommand('mcePre', function () {
    tinymce.activeEditor.formatter.toggle('pre');
  });
  ed.addCommand('mceMd', function () {
    tinymce.activeEditor.formatter.toggle('md');
  });
  ed.addCommand('equationCommand', function (a, latex) {
    if (ed.formatter.match('kbd') || ed.formatter.match('pre') ||
      ed.selection.getContent().match(/.*<tt>.*<\/tt>.*/) != null ||
      ed.selection.getContent().match(/.*<kbd>.*<\/kbd>.*/) != null ||
      ed.selection.getContent().match(/.*<pre>.*<\/pre>.*/) != null) {
      alert("Cannot insert equations within teletype or preformatted text.");
      return;
    }
    // see if user selected existing equation
    var preselect = "";
    var elem = ed.selection.getNode();
    curSelectedLaTeX = null;
    var formula = $(elem).closest(".piazza-latex-edit")
    if (formula.length > 0) {
      curSelectedLaTeX = formula;
      var src = P.text_editor.makeLaTeX(formula.html());
      if (src) {
        preselect = src;
      }
    }

    if ($('#latex_window').html().length == 0) {
      var html = '\
