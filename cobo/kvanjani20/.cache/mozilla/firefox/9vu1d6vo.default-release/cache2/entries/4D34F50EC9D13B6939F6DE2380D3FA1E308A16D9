      EqEditor.init('id', undefined, undefined, 'latexEditor');
      var a = new EqTextArea('latexCancel', 'latexTextArea');
      try {
        EqEditor.add(a, false);
      } catch (err) {
      }
      $('#latexTextArea').keyup(function (e) {
        refreshLatexPreview(false);
      });
      $('#latexEditor').click(function () {
        refreshLatexPreview(true);
      });
    }
    $('#latex_window').show();
    $.fancybox({
      'autoScale': true,
      'hideOnOverlayClick': false,
      'padding': 0,
      'href': '#latex_window',
      'showCloseButton': false,
      'transitionIn': 'none',
      'transitionOut': 'none'
    });

    $('#latexTextArea').val(preselect).focus();
    refreshLatexPreview(true);
  });
  var caretInPre = function(ev) {
    if (ev.parents) {
      var p = ev.parents;
      for (var i = 0; i < p.length; i++) {
        if (p[i].nodeName && p[i].nodeName.toLowerCase() === 'pre') {
          return true;
        }
      }
    }
    return false;
  };
  ed.ui.registry.addToggleButton('equation', {
    tooltip: 'LaTeX Equation Editor',
    text: 'f(x)',
    onAction: function() {
      ed.execCommand('equationCommand');
    },
    onSetup: function(buttonApi) {
      const editorEventCallback = function(eventApi) {
        buttonApi.setDisabled(caretInPre(eventApi));
        buttonApi.setActive(eventApi.element.nodeName.toLowerCase() === 'latex');
        $(eventApi.element).closest("body").find(".piazza-latex-edit").css("background", "#FFF");
        if (buttonApi.isActive) {
          $(eventApi.element).closest(".piazza-latex-edit").css("background", "#FFC");
        }
      }
      ed.on('NodeChange', editorEventCallback);
      return function(buttonApi) {ed.off('NodeChange', editorEventCallback);};
    }
  });
  ed.ui.registry.addButton('insertfile', {
    tooltip: 'Insert file',
    icon: 'browse',
    onAction: function() {ed.execCommand('insertfileCommand');},
    onSetup: function(buttonApi) {
      const editorEventCallback = function(eventApi) {
        buttonApi.setDisabled(caretInPre(eventApi));
      }
      ed.on('NodeChange', editorEventCallback);
      return function(buttonApi) {ed.off('NodeChange', editorEventCallback);};
    }
  });
  ed.ui.registry.addToggleButton('prebutton', {
    tooltip: 'Code block',
    icon: 'code-sample',
    onAction: function() {ed.execCommand('mceToggleFormat', true, "pre");},
    onSetup: function(buttonApi) {
      const editorEventCallback = function(eventApi) {
        buttonApi.setActive(caretInPre(eventApi));
      }
      ed.on('NodeChange', editorEventCallback);
      return function(buttonApi) {ed.off('NodeChange', editorEventCallback);};
    }
  });
  ed.addCommand('mceTt', function () {
    tinymce.activeEditor.formatter.toggle('kbd');
  });
  ed.ui.registry.addToggleButton('ttbutton', {
    text: 'TT',
    tooltip: 'Teletype text ' + (PA.isMac ? '(â‡§âŒ˜M)' : '(Ctrl+Shift+M)'),
    onAction: function() {ed.execCommand('mceTt');},
    onSetup: function(buttonApi) {
      const editorEventCallback = function(eventApi) {
        buttonApi.setDisabled(caretInPre(eventApi));
        return buttonApi.setActive(eventApi.element.nodeName.toLowerCase() === 'kbd' || eventApi.element.nodeName.toLowerCase() === 'tt');
      }
      ed.on('NodeChange', editorEventCallback);
      return function(buttonApi) {ed.off('NodeChange', editorEventCallback);};
    }
  });

  ed.ui.registry.addToggleButton('quotebutton', {
    icon: 'quote',
    tooltip: 'Blockquote ' + (PA.isMac ? '(â‡§âŒ˜9)' : '(Ctrl+Shift+9)'),
    onAction: function() {ed.execCommand('mceBlockQuote');},
    onSetup: function(buttonApi) {
      const editorEventCallback = function(eventApi) {
        buttonApi.setDisabled(caretInPre(eventApi));
        return buttonApi.setActive(eventApi.element.nodeName.toLowerCase() === 'blockquote' || eventApi.element.parentElement.nodeName.toLowerCase() === 'blockquote');
      }
      ed.on('NodeChange', editorEventCallback);
      return function(buttonApi) {ed.off('NodeChange', editorEventCallback);};
    }
  });

  ed.ui.registry.addMenuItem('insertfile', {
    icon: 'browse',
    text: 'File...',
    onAction: function() {ed.execCommand('insertfileCommand');},
  });

  ed.addCommand('formatHelp', function() {
    window.open('/help/formatting.html', '_blank');
  });
  ed.ui.registry.addButton('format_help', {
    tooltip : 'Formatting help',
    title : 'Help',
    icon: 'help',
    onAction: function() {ed.execCommand('formatHelp');},
  });

  ed.addCommand('mcePreview', function() {
    if (ed.id.substring(0, 5) === "rich_") {
      var id = ed.id.substring(5);
      P.text_editor.handlePreviewButton(id, true);
    }
  });
  ed.ui.registry.addButton('preview', {
    tooltip : 'Preview ' + (PA.isMac ? '(âŒ˜P)' : '(Ctrl+P)'),
    title : 'Preview',
    icon: 'preview',
    onAction: function() {ed.execCommand('mcePreview');},
  });
});

function editSelectedLatex() {
  var ed = tinyMCE.activeEditor;
  ed.execCommand("equationCommand");
}

// Add a new placeholder at the actual selection.
function addLatexToEditor() {
  var content_text = $('#latexTextArea').val();
  var initial = content_text;
  if (content_text.length == 0) return false;
  content_text = content_text.replace(/&lt;/g, "<");
  content_text = content_text.replace(/&gt;/g, ">");
  content_text = content_text.replace(/&amp;/g, "&");
  content_text = content_text.replace(/&quot;/g, '"');
  content_text = content_text.replace(/\n$/, "<br/>&nbsp;");
  content_text = content_text.replace(/\n/g, "<br/>");
  var initial_escaped = initial.replace(/>/g, "&#62;");
  initial_escaped = initial_escaped.replace(/</g, "&#60;");
  initial_escaped = initial_escaped.replace(/"/g, "&#34;");
  initial_escaped = initial_escaped.replace(/'/g, "&#39;");
  var latex = "<latex class='piazza-latex-edit' ondblclick='window.parent.editSelectedLatex()'>" + content_text + "</latex>";
  $('#latexTextArea').val("");
  $.fancybox.close();
  tinymce.activeEditor.focus();
  if (typeof (actualCaretPositionBookmark) != 'undefined' && actualCaretPositionBookmark)
    tinymce.activeEditor.selection.moveToBookmark(actualCaretPositionBookmark);
  if (curSelectedLaTeX) {
    curSelectedLaTeX.remove();
  } else {
    latex = "$$" + latex + "$$";
  }
  tinyMCE.activeEditor.execCommand('mceInsertContent', false, latex);
  return false;
};

function cancelLatex() {
  $('#latexTextArea').val("");
  $.fancybox.close();
  return false;
}
